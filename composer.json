{
  "name": "parkdoug",
  "description": "WordPress theme for parkdoug.com",
  "authors": [
    {
      "name": "Sean McHenry",
      "email": "seankmchenry@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "packages"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "jjgrainger/wp-custom-post-type-class": "*",
    "tgmpa/tgm-plugin-activation": "*",
    "wpackagist-plugin/force-regenerate-thumbnails": "*",
    "wpackagist-plugin/imsanity": "*"
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}": ["type:wordpress-plugin"],
      "inc/vendor/{$name}": [
        "tgmpa/tgm-plugin-activation",
        "jjgrainger/wp-custom-post-type-class"
      ]
    }
  }
}